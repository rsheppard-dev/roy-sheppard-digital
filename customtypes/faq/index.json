{
  "id": "faq",
  "label": "FAQ",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "faqList": {
        "type": "Group",
        "config": {
          "label": "FAQ",
          "fields": {
            "question": {
              "type": "Text",
              "config": {
                "label": "Question",
                "placeholder": ""
              }
            },
            "answer": {
              "type": "StructuredText",
              "config": {
                "label": "Answer",
                "placeholder": "",
                "allowTargetBlank": true,
                "multi": "paragraph,preformatted,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
              }
            },
            "catagory": {
              "type": "Select",
              "config": {
                "label": "Category",
                "placeholder": "",
                "options": [
                  "Generic",
                  "Web Design",
                  "Web Development",
                  "E-Commerce"
                ],
                "default_value": "Generic"
              }
            }
          }
        }
      }
    }
  }
}